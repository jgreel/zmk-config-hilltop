#include <dt-bindings/zmk/mouse.h>

#define ALPHA 0
#define SYM 1
#define NUM 2
#define MOUSE 3

/ {
	behaviors {
		ht_tp: hold_tap_tap_preferred {
			compatible = "zmk,behavior-hold-tap";
    		label = "HOLD_TAP_TAP_PREFERRED";
    		#binding-cells = <2>;
    		flavor = "tap-preferred";
    		tapping-term-ms = <200>;
    		quick-tap-ms = <150>;
    		bindings = <&kp>, <&kp>;
		};

		ht_bal: hold_tap_balanced {
			compatible = "zmk,behavior-hold-tap";
    		label = "HOLD_TAP_BALANCED";
    		#binding-cells = <2>;
    		flavor = "balanced";
    		tapping-term-ms = <200>;
    		quick-tap-ms = <150>;
    		bindings = <&kp>, <&kp>;
		};
    };

	combos {
		compatible = "zmk,combos";
		combo_lbrkt {
            timeout-ms = <50>;
            key-positions = <40 41>;
            bindings = <&kp LBKT>;
        };
		combo_rbrkt {
            timeout-ms = <50>;
            key-positions = <56 57>;
            bindings = <&kp RBKT>;
        };
		combo_alpha {
            timeout-ms = <50>;
            key-positions = <63 68>;
            bindings = <&to ALPHA>;
        };
		combo_mouse_left {
			timeout-ms = <50>;
			key-positions = <49 65>;
			bindings = <&mo MOUSE>;
		};
		combo_mouse_right {
			timeout-ms = <50>;
			key-positions = <50 66>;
			bindings = <&mo MOUSE>;
		};
	};

